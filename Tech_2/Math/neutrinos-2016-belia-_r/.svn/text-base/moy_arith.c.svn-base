#include	<math.h>
#include	"206neutrinos.h"

double	moy_arith(t_variable *_var, t_result *_res, double nbAdd)
{
  _res->newMoyArith = ((_var->somme + nbAdd) / (_res->newNbMesures));
  return (_res->newMoyArith);
}
